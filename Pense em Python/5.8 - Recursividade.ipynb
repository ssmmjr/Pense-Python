    "#Recursividade\n",
    "from time import sleep\n",
    "def cont_reg(n):\n",
    "    if n <= 0:\n",
    "        sleep(1)\n",
    "        print('Blastoff!!!!')\n",
    "    else:\n",
    "        sleep(1)\n",
    "        print(n)\n",
    "        cont_reg(n - 1)\n",
    "\n",
    "num = int(input('Digite um número inteiro qualquer: '))\n",
    "cont_reg(num)"
   ]
  }
